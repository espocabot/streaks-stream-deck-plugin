{
	"compilerOptions": {
		"lib": ["DOM", "ES2022"],
		"customConditions": ["node"],
		"module": "ES2022",
		"target": "es2022",
		"moduleResolution": "Bundler",
		"noImplicitOverride": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*.ts", "global.d.ts"],
	"exclude": ["node_modules"]
}
